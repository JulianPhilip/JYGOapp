<h1> Liste des users </h1>


<table style="width:100%">
  <thead>
  <tr>
    <th>Firstname</th>
    <th>Lastname</th>
    <th>Username</th>
    <th>Email</th>
    <th>Adresse</th>
    <th>Zipcode</th>
    <th>Ville</th>
  </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.firstname %></td>
    <td><%= user.lastname %></td>
    <td><%= user.username %></td>
    <td><%= user.email %></td>
    <td><%= user.address %></td>
    <td><%= user.zip_code %></td>
    <td><%= user.city %></td>
  </tr>
  <% end %>
  </tbody>
</table>

